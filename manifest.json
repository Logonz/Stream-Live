{
   "background": {
      "persistent": true,
      "scripts": [ "scripts/jquery-1.6.1.min.js", "scripts/mixer/carina.js", "scripts/background.js","scripts/mixer/mixer.js" ]
   },
   "browser_action": {
      "default_icon": "images/icon.png",
      "default_popup": "popup.html",
      "default_title": "Stream Live"
   },
   "description": "Extension for displaying a user's favorite channels on Twitch.tv and Mixer which are currently live.",
   "homepage_url": "http://www.mikechambers.com/blog/2011/07/06/jtv-live-a-google-chrome-extension-for-justin-tv/",
   "icons": {
      "128": "images/icon-128.png",
      "16": "images/icon-16.png",
      "48": "images/icon-48.png"
   },
   "manifest_version": 2,
   "name": "Stream Live",
   "options_page": "options.html",
   "permissions": [ "https://api.twitch.tv/", "https://mixer.com/api/", "notifications", "contextMenus" ],
   "version": "1.88",
   "web_accessible_resources": [ "images/icon.png" ]
}
